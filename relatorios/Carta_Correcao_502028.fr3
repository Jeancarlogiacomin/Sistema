<TfrxReport Tag="1" Version="4.15.13" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="3583" PreviewOptions.Zoom="1" PrintOptions.Printer="Padrão" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40239,3775247685" ReportOptions.Description.Text="Versão 2.0 - Visualização da Carta de Correção Eletrônica" ReportOptions.Name="Carta Correção" ReportOptions.LastChange="42446,7472246528" ReportOptions.VersionBuild="0" ReportOptions.VersionMajor="3" ReportOptions.VersionMinor="0" ReportOptions.VersionRelease="1" ScriptLanguage="PascalScript" ScriptText.Text="var statusCCe,tipodocumento, datadoc, numero, transp:String;&#13;&#10;                    &#13;&#10;  &#13;&#10;procedure PageHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if (&#60;QueryCorrecao.&#34;DOCUMENTO_TIPO&#34;&#62;=11) then begin          &#13;&#10;     tipodocumento:='Conhecimento de Transporte Internacional por Rodovia (CRT)';&#13;&#10;     QueryCRT.close;&#13;&#10;     QueryCRT.sql.text:=' select parceiro_negocio.nome, crt.numero_crt_2, crt.data_emissao from crt'+&#13;&#10;                        ' inner join parceiro_negocio on parceiro_negocio.id=crt.transportador_id'+                                                                                                                                                                                                                                                                                                                                                     &#13;&#10;                        ' where CRT.id='+inttostr(&#60;QueryCorrecao.&#34;DOCUMENTO_ID&#34;&#62;);                                                                                                                                                                                                                                                                                                                                                                                                  &#13;&#10;     QueryCRT.open;&#13;&#10;     numero:=(&#60;QueryCRT.&#34;NUMERO_CRT_2&#34;&#62;);&#13;&#10;     datadoc:=(&#60;QueryCRT.&#34;DATA_EMISSAO&#34;&#62;);&#13;&#10;     transp:=(&#60;QueryCRT.&#34;NOME&#34;&#62;);                               &#13;&#10; end;  &#13;&#10; if (&#60;QueryCorrecao.&#34;DOCUMENTO_TIPO&#34;&#62;=12) then begin&#13;&#10;     tipodocumento:='Manifesto Internacional de Carga Rodoviária (MIC/DTA)';                                                            &#13;&#10;     QueryMIC.close;&#13;&#10;     QueryMIC.sql.text:=' select parceiro_negocio.nome, mic.numero_mic_4, mic.data_emissao_6 from mic'+&#13;&#10;                        ' inner join parceiro_negocio on parceiro_negocio.id=mic.transportador_id'+                                                                                                                                                                                                                                                                                                                                            &#13;&#10;                        ' where mic.id='+inttostr(&#60;QueryCorrecao.&#34;DOCUMENTO_ID&#34;&#62;);&#13;&#10;     QueryMIC.open;                                                             &#13;&#10;     numero:=(&#60;QueryMIC.&#34;NUMERO_MIC_4&#34;&#62;);&#13;&#10;     datadoc:=(&#60;QueryMIC.&#34;DATA_EMISSAO_6&#34;&#62;);&#13;&#10;     transp:=(&#60;QueryMIC.&#34;NOME&#34;&#62;);                               &#13;&#10; end;&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;procedure frxReport1OnStartReport(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  QueryCorrecao.close;&#13;&#10;  QueryCorrecao.sql.text:=' select first 1 carta_correcao.*,documento.chave_nfe, ctrc.chave_cte from carta_correcao '+  &#13;&#10;                          ' left join documento on documento.id=carta_correcao.documento_id and (documento.tipo=1 or documento.tipo=0) '+&#13;&#10;                          ' left join ctrc on ctrc.id=carta_correcao.documento_id '+                                          &#13;&#10;                          &#60;PARAMS&#62;+&#13;&#10;                          ' and carta_correcao.status_doc_eletronico=2 '+&#13;&#10;                          ' order by carta_correcao.num_ordem desc ';&#13;&#10;  QueryCorrecao.open;&#13;&#10;&#13;&#10;  QueryItensCorrecao.close;&#13;&#10;  QueryItensCorrecao.sql.text:='select id, grupo_alterado, campo_alterado, nro_item_alterado, valor_alterado from carta_correcao_itens '+       &#13;&#10;                               ' where carta_correcao_id='+intToStr(&#60;QueryCorrecao.&#34;ID&#34;&#62;);&#13;&#10;  QueryItensCorrecao.Open;                                                 &#13;&#10;    &#13;&#10;  PageCC.Visible:=(&#60;QueryCorrecao.&#34;STATUS_DOC_ELETRONICO&#34;&#62;=0);&#13;&#10;  PageCCe.Visible:=(&#60;QueryCorrecao.&#34;STATUS_DOC_ELETRONICO&#34;&#62;=2);                                        &#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin &#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure PageHeader2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var ctenfe:string;&#13;&#10;begin&#13;&#10;&#13;&#10;  if &#60;QueryCorrecao.&#34;STATUS_DOC_ELETRONICO&#34;&#62;=2 then&#13;&#10;  begin              &#13;&#10;    if (&#60;QueryCorrecao.&#34;DOCUMENTO_TIPO&#34;&#62;=6) then // CT-e&#13;&#10;    begin                        &#13;&#10;      ctenfe:='O CT-E';  &#13;&#10;      BarCode2.Text:=&#60;QueryCorrecao.&#34;CHAVE_CTE&#34;&#62;;&#13;&#10;      memo14.text:=BarCode2.Text;                                                            &#13;&#10;      memo29.text:='CHAVE DE ACESSO DO CT-E';&#13;&#10;    end else&#13;&#10;    begin              &#13;&#10;      ctenfe:='A NF-E';&#13;&#10;      BarCode2.Text:=&#60;QueryCorrecao.&#34;CHAVE_NFE&#34;&#62;;                                                          &#13;&#10;      memo14.text:=BarCode2.Text;  &#13;&#10;      memo29.text:='CHAVE DE ACESSO DA NF-E';                                        &#13;&#10;    end;&#13;&#10;  end;&#13;&#10;    &#13;&#10;  Memo35.text:=copy(BarCode2.Text,21,2);                                                                  &#13;&#10;  Memo37.text:=copy(BarCode2.Text,23,3);                                                                  &#13;&#10;  Memo39.text:=copy(BarCode2.Text,26,9);                                                                  &#13;&#10;  Memo41.text:=copy(BarCode2.Text,5,2)+'/'+&#13;&#10;               '20'+copy(BarCode2.Text,3,2);&#13;&#10;                                          &#13;&#10;      &#13;&#10;      &#13;&#10;  if &#60;QueryCorrecao.&#34;STATUS_DOC_ELETRONICO&#34;&#62;=2 then&#13;&#10;    statusCCe:='EVENTO REGISTRADO E VINCULADO COM '+ctenfe              &#13;&#10;  else if &#60;QueryCorrecao.&#34;STATUS_DOC_ELETRONICO&#34;&#62;=4 then&#13;&#10;    statusCCe:='EVENTO REGISTRADO E NÃO VINCULADO COM '+ctenfe                &#13;&#10;  else                  &#13;&#10;    statusCCe:='****************  EVENTO NÃO REGISTRADO  ****************'&#13;&#10;    &#13;&#10;    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Memo24.Color:=ZebraCor(Memo24.Color);                                                                              &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;    &#13;&#10;end." OnStartReport="frxReport1OnStartReport" PropData="">
