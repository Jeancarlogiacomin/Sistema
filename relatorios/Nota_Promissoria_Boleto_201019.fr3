<TfrxReport Tag="1" Version="4.15.11" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="3583" PreviewOptions.Zoom="1" PrintOptions.Printer="Padrão" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40715,7094932523" ReportOptions.Description.Text="" ReportOptions.Name="Nota Promissoria" ReportOptions.LastChange="41893,6002102662" ReportOptions.VersionBuild="0" ReportOptions.VersionMajor="1" ReportOptions.VersionMinor="0" ReportOptions.VersionRelease="0" ScriptLanguage="PascalScript" ScriptText.Text="var codigoBanco,MensagemPagamento,Carteira,NossoNumero:string;                                                                 &#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  queryBoleto.close;&#13;&#10;  queryBoleto.sql.text:='select boleto.*, extract (day from vencimento) dia_vencimento, extract (year from vencimento) ano_vencimento, '+       &#13;&#10;       ' cb.conta_corrente, cb.dig_conta_corrente,cb.convenio, cb.modalidade_conta,cb.codigo_cedente, '+       &#13;&#10;       ' ag.agencia,ag.dig_agencia,ag.codigo_banco, '+       &#13;&#10;       ' cedente.registrado_para nome_cedente, cedente.cnpj cedente_cnpj, '+  &#13;&#10;       ' sacado.nome,sacado.tipo_pessoa,sacado.cnpj,sacado.cpf, '+  &#13;&#10;       ' endereco_sacado.logradouro,endereco_sacado.bairro,endereco_sacado.cep, '+  &#13;&#10;       ' endereco_sacado.nome_cidade,endereco_sacado.uf, endereco_sacado.numero endereco_sacado_numero, '+  &#13;&#10;       ' endereco_sacado.complemento '+  &#13;&#10;&#13;&#10;       ' from boleto '+  &#13;&#10;&#13;&#10;       ' inner join V_CONTA_BANCARIA vcb on vcb.id=boleto.conta_bancaria_id '+  &#13;&#10;       ' inner join conta_bancaria cb on cb.id=vcb.id '+  &#13;&#10;       ' inner join agenciabancaria ag on ag.parceiro_negocio_id=cb.agenciabancaria_id '+  &#13;&#10;       ' inner join empresa cedente on cedente.id=boleto.empresa_cedente_id '+  &#13;&#10;       ' inner join parceiro_negocio sacado on sacado.id=boleto.sacado_id '+  &#13;&#10;       ' inner join v_endereco_parceiro endereco_sacado on endereco_sacado.id=boleto.endereco_sacado_id '+  &#13;&#10;       ' where boleto.id='+GetParamValue(&#60;PARAMS&#62;,'ID')+                  &#13;&#10;       ' order by id';&#13;&#10;  queryBoleto.open;&#13;&#10;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page1OnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if queryboleto.FieldByName('codigo_banco').AsString='001' then                             &#13;&#10;    ShowMessage('Atenção: Boleto ainda não homologado para este banco.'+#13+&#13;&#10;                'Favor realizar os testes e validações junto a sua agência antes de usar em produção.'+#13+#13+        &#13;&#10;                'Ao validar o boleto favor enviar o OK da validação para o suporte para removermos esta mensagem.');&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  MensagemPagamento:='Pagamento em qualquer agência bancária ou lotéricas até o vencimento.';&#13;&#10;  carteira:=&#60;QueryBoleto.&#34;CARTEIRA&#34;&#62;;&#13;&#10;  NossoNumero:=&#60;QueryBoleto.&#34;NUMEROBANCARIO&#34;&#62;;              &#13;&#10;  if queryboleto.FieldByName('codigo_banco').AsString='001' then begin&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;    codigoBanco:='001-9';&#13;&#10;    NossoNumero:=&#60;QueryBoleto.&#34;CONVENIO&#34;&#62;+formatfloat('0000000000',&#60;QueryBoleto.&#34;NOSSO_NUMERO&#34;&#62;);                                                                  &#13;&#10;    MensagemPagamento:=MensagemPagamento+' Após somente no Banco do Brasil.';                                                                                                                                  &#13;&#10;  end else if queryboleto.FieldByName('codigo_banco').AsString='104' then begin&#13;&#10;&#13;&#10;&#13;&#10;                                                                        &#13;&#10;    codigoBanco:='104-0';                                                &#13;&#10;    MensagemPagamento:=MensagemPagamento+' Após, somente na Caixa.';&#13;&#10;    if carteira='24' then&#13;&#10;      carteira:='SR';                                                                &#13;&#10;  end else if queryboleto.FieldByName('codigo_banco').AsString='237' then begin&#13;&#10;&#13;&#10;&#13;&#10;                                                                        &#13;&#10;    codigoBanco:='237-2';                                                &#13;&#10;    MensagemPagamento:=MensagemPagamento+' Após, somente nas agências do Bradesco.';&#13;&#10;  end else if queryboleto.FieldByName('codigo_banco').AsString='341' then begin&#13;&#10;&#13;&#10;&#13;&#10;                                                                        &#13;&#10;    codigoBanco:='341-7';                                                &#13;&#10;    MensagemPagamento:=MensagemPagamento+' Após, somente nas agências do Itaú.';&#13;&#10;  end else if queryboleto.FieldByName('codigo_banco').AsString='748' then begin&#13;&#10;&#13;&#10;&#13;&#10;                                                                        &#13;&#10;    codigoBanco:='748-X';                                                &#13;&#10;    MensagemPagamento:=MensagemPagamento+' Após, somente nas agências do Sicredi.';&#13;&#10;  end&#13;&#10;        &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." PropData="044C65667403780203546F700388000844617461736574730101063020446174615365743D225175657279426F6C65746F2220446174615365744E616D653D225175657279426F6C65746F220000095661726961626C65730100055374796C650100">
