orrente cb_dig_conta_corrente, '+       &#13;&#10;                               ' banco.nome banco_nome, ag.agencia ag_agencia, ag.dig_agencia ag_dig_agencia, '+       &#13;&#10;                               ' fat_Reg.instrucoes fat_Reg_instrucoes, fat_Reg.numero fat_Reg_numero,parcelafatura.id '+                &#13;&#10;                          &#13;&#10;                        ' from faturamento '+  &#13;&#10;                        ' inner join parcelafatura on parcelafatura.faturamento_id=faturamento.id '+&#13;&#10;                        ' left join faturamento_registro fat_Reg on fat_Reg.faturamento_id=faturamento.id '+                                   &#13;&#10;                        ' left join parceiro_negocio on parceiro_negocio.id=fat_reg.parceiro_negocio_id '+  &#13;&#10;                        ' left join endereco_parceiro on endereco_parceiro.parceiro_negocio_id=parceiro_negocio.id and endereco_parceiro.endereco_principal = 1'+  &#13;&#10;                        ' left join cidade on cidade.id=endereco_parceiro.cidade_id'+&#13;&#10;                        ' left join conta_bancaria cb on cb.id=fat_Reg.conta_bancaria_id '+&#13;&#10;                        ' left join parceiro_negocio banco on banco.id=cb.agenciabancaria_id '+&#13;&#10;                        ' left join agenciabancaria ag on ag.parceiro_negocio_id=banco.id '+  &#13;&#10;                            &#13;&#10;                        ' where faturamento.id='+GetParamValue(&#60;PARAMS&#62;,'ID');                                              &#13;&#10;  QueryFatura.open;&#13;&#10;&#13;&#10;  IBOQueryaux.close;&#13;&#10;  IBOQueryaux.sql.text:=' select count (parcelafatura.id) from faturamento'+    &#13;&#10;                        ' inner join parcelafatura on parcelafatura.faturamento_id=faturamento.id '+&#13;&#10;                        ' where faturamento.id='+GetParamValue(&#60;PARAMS&#62;,'ID');                            &#13;&#10;  IBOQueryaux.open;                    &#13;&#10;&#13;&#10;  Memo22.text:=formatFloat('000000',QueryFatura.FieldByName('FAT_REG_NUMERO').AsInteger);&#13;&#10;&#13;&#10;  if &#60;QueryFatura.&#34;TIPO&#34;&#62;=1 then begin&#13;&#10;     memo35.text:='FATURA';                                                                  &#13;&#10;     memo23.text:='';&#13;&#10;     memo2.text:='Cliente:';&#13;&#10;     memo27.text:='FAVOR EFETUAR ORDEM DE PAGAMENTO EM:'+#13+          &#13;&#10;                  'BANCO: [QueryFatura.&#34;BANCO_NOME&#34;]'+#13+    &#13;&#10;                  'AGÊNCIA: [QueryFatura.&#34;AG_AGENCIA&#34;] - [QueryFatura.&#34;AG_DIG_AGENCIA&#34;]'+#13+    &#13;&#10;                  'CONTA CORRENTE: [QueryFatura.&#34;CB_CONTA_CORRENTE&#34;] - [QueryFatura.&#34;CB_DIG_CONTA_CORRENTE&#34;]'+#13+    &#13;&#10;                  '[DBDataSetEmpresa.&#34;NOME&#34;]'+#13+    &#13;&#10;                  '[DBDataSetEmpresa.&#34;CNPJ&#34;]'+#13+        &#13;&#10;                  '[QueryFatura.&#34;FAT_REG_INSTRUCOES&#34;]';    &#13;&#10;  end else                          &#13;&#10;  if &#60;QueryFatura.&#34;TIPO&#34;&#62;=2 then begin&#13;&#10;     memo35.text:='FATURA DE DESPESA';                                                                     &#13;&#10;     memo23.text:='(Autorização de Pagamento)';&#13;&#10;     memo2.text:='Fornecedor:';                                       &#13;&#10;  end;  &#13;&#10;  &#13;&#10;  &#13;&#10;          &#13;&#10;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;    &#13;&#10;begin&#13;&#10;                                        &#13;&#10;  QueryItensFatura.close;&#13;&#10;  QueryItensFatura.sql.text:=' select itemfinanceiro.*,'+&#13;&#10;                                    ' veiculo.placa, ctrc.icms_valor,'+  &#13;&#10;                                    ' financeiro.num_doc, financeiro.data_doc,'+        &#13;&#10;                                    ' parcela.num_doc,'+                                                                                           &#13;&#10;                                    ' parcela.valor,'+    &#13;&#10;                                    ' parcela.data_prog_venc'+&#13;&#10;                             ' from itemfinanceiro '+&#13;&#10;                             ' inner join parcela on parcela.financeiro_id=itemfinanceiro.financeiro_id'+                               &#13;&#10;                             ' inner join financeiro on financeiro.id=itemfinanceiro.financeiro_id '+   &#13;&#10;                             ' left join ctrc on ctrc.financeiro_id=itemfinanceiro.financeiro_id'+  &#13;&#10;                             ' left join transporte on transporte.id=itemfinanceiro.transporte_id'+&#13;&#10;                             ' left join veiculo on veiculo.id=transporte.veiculo1_id '+                &#13;&#10;  &#13;&#10;                             ' where parcela.faturamento_id='+GetParamValue(&#60;PARAMS&#62;,'ID')+&#13;&#10;                             ' order by financeiro.num_doc';&#13;&#10;                                 &#13;&#10;  QueryItensFatura.open;&#13;&#10;&#13;&#10;  contaparcela:=1+contaparcela;             &#13;&#10;      &#13;&#10;end;&#13;&#10;           &#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var notas:string; ctrc_id_ant:integer;                                                                              &#13;&#10;begin&#13;&#10;  Memo13.Color:=ZebraCor(Memo13.Color);&#13;&#10;  Memo31.Color:=Memo13.Color;                                                        &#13;&#10;  QueryNFCTRC.close;                  &#13;&#10;  QueryNFCTRC.sql.text:=' select ctrc_nf.peso_liquido,ctrc_nf.peso_bruto, ctrc_nf.numero, '+&#13;&#10;                               ' ctrc_nf.serie from ctrc '+&#13;&#10;                        ' inner join ctrc_nf on ctrc_nf.ctrc_id=ctrc.id '+&#13;&#10;                        ' where ctrc.financeiro_id='+intToStr(QueryItensFatura.FieldbyName('financeiro_id').AsInteger);                                                                          &#13;&#10;  QueryNFCTRC.open;&#13;&#10;  notas:='';                        &#13;&#10;  if not QueryNFCTRC.IsEmpty then begin&#13;&#10;    while not QueryNFCTRC.Eof do begin&#13;&#10;      Notas:=Notas+' NF '+FormatFloat('000000',QueryNFCTRC.FieldByName('numero').AsInteger)+&#13;&#10;             ' Série '+QueryNFCTRC.FieldByName('serie').AsString+&#13;&#10;             ' / Peso B.: '+FormatFloat('0.000',QueryNFCTRC.FieldByName('peso_bruto').AsFloat)+                                            &#13;&#10;             ' - Peso L.: '+FormatFloat('0.000',QueryNFCTRC.FieldByName('peso_liquido').AsFloat)+' | ';                                              &#13;&#10;      QueryNFCTRC.Next;                         &#13;&#10;    end;&#13;&#10;    Notas := BuscaTroca(Notas,'[',' ');&#13;&#10;    Notas := BuscaTroca(Notas,']',' ');&#13;&#10;    Notas := BuscaTroca(Notas,'&#60;',' ');&#13;&#10;    Notas := BuscaTroca(Notas,'&#62;',' ');        &#13;&#10;    Memo31.Text:='     Ref.: '+Notas;&#13;&#10;  end else&#13;&#10;    //Child1.Visible:=False;&#13;&#10;    Memo31.Text:='     Documento: '+(&#60;QueryItensFatura.&#34;DESCRICAO&#34;&#62;)+'  Quant.: '+FormatFloat('0.000',QueryItensFatura.FieldByName('quantidade').AsFloat);        &#13;&#10;&#13;&#10;  &#13;&#10;  counttotal:=counttotal+contaparcela;  &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;procedure Footer1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;QueryFatura.&#34;TIPO&#34;&#62;=2 then begin&#13;&#10;     memo33.visible:=true;&#13;&#10;     memo34.visible:=true;                                                 &#13;&#10;  end else begin&#13;&#10;     memo33.visible:=false;&#13;&#10;     memo34.visible:=false;  &#13;&#10;  end;  &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure Header2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Memo13.color:=clWhite;                                              &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." OnReportPrint="frxReportOnReportPrint" PropData="">
